<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.LoginController" spacing="10">
    <Label text="Login" />
    <TextField fx:id="emailField" promptText="Email" maxWidth="200"/>
    <PasswordField fx:id="passwordField" promptText="Password" maxWidth="200" />
    
    
    <HBox spacing="10">
        <Button text="Login" onAction="#handleLogin" />
    	<Button text="Register" onAction="#handleRegister" />
    </HBox>
</VBox>
